{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "Polymer": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eqeqeq": [2, "smart"],
    "space-after-keywords": 2,
    "no-console": 1,
    "no-new": [0]
  }
}
